    "private": true,
    "scripts": {
        "dev": "next dev -p 3001",
        "build": "next build",
        "start": "next start -p 3001",
        "lint": "next lint --dir src",
        "lint:fix": "npm run lint -- --fix",
        "pretty": "npx prettier --write",
        "pretty:all": "npm run pretty .",
        "prepare": "husky install",
        "types:generate-scss": "npx typed-scss-modules src",
        "types:generate-scss-watch": "npx typed-scss-modules src --watch",
        "typecheck": "tsc",
        "typecheck:watch": "tsc -w",
        "test": "jest --maxWorkers=1",
        "test:detect": "npm run test --detectOpenHandles",
        "test:silent": "npm run test --silent",
        "test:coverage": "npm run test --collect-coverage"
    },
